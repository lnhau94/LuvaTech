                <Label layoutX="149.0" layoutY="14.0" prefHeight="39.0" prefWidth="243.0" styleClass="background-staffinfor" text="Customer Edit" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
                <Label layoutX="14.0" layoutY="75.0" prefHeight="27.0" prefWidth="121.0" styleClass="title-staffadd" text="Full Name" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label layoutX="14.0" layoutY="119.0" prefHeight="27.0" prefWidth="121.0" styleClass="title-staffadd" text="Birth Day" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="customerPhone" layoutX="149.0" layoutY="163.0" prefHeight="27.0" prefWidth="243.0" />
