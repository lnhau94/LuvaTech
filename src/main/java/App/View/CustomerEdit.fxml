<AnchorPane prefHeight="400.0" prefWidth="412.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controller.CustomerEdit">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="412.0" styleClass="background-staffadd" stylesheets="@css/staff.css">
