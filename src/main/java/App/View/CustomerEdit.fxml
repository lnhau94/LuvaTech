                <TextField fx:id="customerDob" layoutX="149.0" layoutY="119.0" prefHeight="27.0" prefWidth="243.0" />
                <Label layoutX="16.0" layoutY="208.0" prefHeight="27.0" prefWidth="121.0" styleClass="title-staffadd" text="Address" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            <TextField fx:id="customerName" layoutX="149.0" layoutY="75.0" prefHeight="27.0" prefWidth="243.0" />
